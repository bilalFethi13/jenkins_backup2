    <script>pipeline {
    agent any

    tools {
        maven &quot;MAVEN&quot;
    }

    stages {
        stage(&apos;Build &amp; Unit test&apos;){
            steps {
                // Run maven clean and package
                sh &apos;sudo mvn clean package&apos;

                // Publish JUnit test results
                junit &apos;**/target/surefire-reports/TEST-*.xml&apos;
            }
        }

        stage(&apos;Security SAT : OWASP Dependency Check&apos;){
            steps {
                //Run AWASP Dependency Check
                sh &quot;sudo mvn verify&quot;
            }
        }

        stage(&apos;Deploy to tomcat&apos;){
            steps{
                script {
                    // Deploy to tomcat
                    sh &apos;copy target/MyWebAppSecurity.war /opt/tomcat/webapps/mywebapp.war&apos;
                }
            }
        }

        stage(&apos;Scan ZAP(Active Scan DAST)&apos;){
            steps {
                script {
                    // Run ZAP Docker container for Active Scan elevated privileges

                    sh &apos;docker run --rm -v /home/vagrant:/zap/wrk/:rw -t ghcr.io/zaproxy:stable zap-baseline.py -t http://52.143.189.38:8080/mywebapp/ -x report_OWASP || exit 0&apos;
                }
            }
        }
    }
}</script>
